	"tokenColors": [
		{
			"name": "Main",
			"scope": [
				"comment"
			],
			"settings": {
				"foreground": "~main~"
			}
		},
		{
			"name": "LightMain",
			"scope": [
				"constant.language",
				"support.function",
				"support.type",
				"keyword.operator.new",
				"variable.language",
				"meta.object-literal.key",
        "entity.name"
			],
			"settings": {
				"foreground": "~lightMain~"
			}
		},
		{
			"name": "String",
			"scope": [
				"string",
				"constant.numeric",
				"meta.structure.dictionary.json string.quoted.double.json"
			],
			"settings": {
				"foreground": "~string~"
			}
		},
		{
			"name": "White",
			"scope": [
				"constant.character",
				"constant.other",
				"variable",
				"entity.other.inherited-class",
				"punctuation.separator.key-value"
			],
			"settings": {
				"foreground": "~white~"
			}
		},
		{
			"name": "Purple",
			"scope": [
				"punctuation.definition.template-expression",
				"punctuation.section.embedded",
				"support.class"
			],
			"settings": {
				"foreground": "~purple~"
			}
		},
		{
			"name": "Blue",
			"scope": [
				"keyword",
				"entity.name.tag",
				"punctuation.definition.tag"
			],
			"settings": {
				"foreground": "~blue~"
			}
		},
		{
			"name": "Red",
			"scope": [
				"punctuation.definition.string",
				"keyword.operator",
				"entity.name.type",
				"entity.name.class",
				"entity.name.namespace",
				"entity.name.scope-resolution",
				"support.constant"
			],
			"settings": {
				"foreground": "~red~"
			}
		},
		{
			"name": "Green",
			"scope": [
				"storage",
				"entity.other.attribute-name"
			],
			"settings": {
				"foreground": "~green~"
			}
		},
		{
			"name": "Yellow",
			"scope": [
				"entity.name.function"
			],
			"settings": {
				"foreground": "~yellow~"
			}
		},
		{
			"name": "Bold",
			"scope": [
				"constant.language",
				"entity.name.type",
				"entity.name.class",
				"entity.name.namespace",
				"entity.name.scope-resolution",
				"support.constant",
				"variable.language"
			],
			"settings": {
				"fontStyle": "bold"
			}
		},
		{
			"name": "Italic",
			"scope": [
				"storage",
				"support.type"
			],
			"settings": {
				"fontStyle": "italic"
			}
		}
	]
}
